# Frontend build-time API base URL (use when NOT using the dev proxy)
# e.g. VITE_API_URL=https://api.example.com
VITE_API_URL=

# Dev server proxy target (overrides default http://localhost:3001)
# Used only by Vite dev server to forward /api requests
VITE_API_PROXY=

# Optional custom dev host for HMR (default 'fichiers')
VITE_DEV_HOST=

# Dev server port (default 80 if unset). On Windows, utiliser un terminal Administrateur ou lib√©rer le port 80.
VITE_DEV_PORT=

# Default PIN shown in settings after auth
VITE_DEFAULT_PIN=Gilles29@

# Storage quota in MB for circular progress (default 1024 if unset)
# Example: 2048 means 2 GB
VITE_STORAGE_QUOTA_MB=1024
